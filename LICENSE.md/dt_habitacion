CREATE TABLE `habitaciones`.`dt_habitacion` ( `idDt_Habitacion` INT NOT NULL AUTO_INCREMENT , `Etage` INT NOT NULL , `NombreHabitacion` INT NOT NULL , `Prix` FLOAT NOT NULL , `Estatus` ENUM('LIBRE','OCUPADA','RESERVADA','PEDIDO','ASEO','REPARACION') NOT NULL DEFAULT 'LIBRE' , `DateEntree` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP() , `DateSortie` TIMESTAMP NOT NULL , PRIMARY KEY (`idDt_Habitacion`)) ENGINE = InnoDB;